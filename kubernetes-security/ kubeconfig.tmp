clusters:
- cluster:
    certificate-authority-data: $CA_CRT
    server: $APISERVER
  name: default-cluster
contexts:
- context:
    cluster: default-cluster
    namespace: homework
    user: cd
  name: default-context
current-context: default-context
kind: Config
preferences: {}
users:
- name: cd
  user:
    token: $TOKEN